msgid "Use any of the following values: `{0}`."
msgstr "Utilitzeu qualsevol dels valors següents: `{0}`."

#: ../plugins/Highway_Parking_Lane.py:76
msgid "Bad parking:condition:[side] value"
msgstr "Valor de parking:condition:[side] incorrecte"

#: ../plugins/Highway_Parking_Lane.py:176
#: ../plugins/Highway_Parking_Lane.py:178
#: ../plugins/Highway_Parking_Lane.py:193
#: ../plugins/Highway_Parking_Lane.py:228
#: ../plugins/Highway_Parking_Lane.py:230
